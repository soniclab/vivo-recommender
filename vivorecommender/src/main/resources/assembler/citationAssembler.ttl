@prefix ja:		<http://jena.hpl.hp.com/2005/11/Assembler#> .
@prefix sonic:	<http://ciknow1.northwestern.edu/sonic#> .
@prefix files:	<ontology/> .

sonic:citationModel
	ja:ontModelSpec ja:OWL_MEM_MICRO_RULE_INF ;
    	
	ja:baseModel [
		a ja:MemoryModel ;

    	ja:content [ja:externalContent
    	
    		# OWL
    		files:citation.owl ,
	
			# instances
			files:citation.rdf
			
			];
			
		ja:prefixMapping [
			ja:prefix "ml";
			ja:namespace "http://research.icts.uiowa.edu/ontology/medline.rdf#"
			];
    	
		ja:prefixMapping [
			ja:prefix "xsd" ;
			ja:namespace "http://www.w3.org/2001/XMLSchema#"
			];
    	
    	]
    .